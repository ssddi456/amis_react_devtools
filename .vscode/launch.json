{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "sourceMapPathOverrides": {
                "webpack://@amis-devtools/sql-devtools-vscode-extension/*": "${workspaceFolder}/packages/sql-devtools-vscode-extension/*"
            },
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}/packages/sql-devtools-vscode-extension"
            ],
            "preLaunchTask": "build extension"
        }
    ]
}